<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="data.mapper.CityInfoMapper">
	
	<!-- 도시 지역번호 -->
	<select id="getData" parameterType="int" resultType="weather">
		select * from city where num=#{num}
	</select>
	
	<!--  -->
	<select id="getName" parameterType="String">
		select * from city where placename=#{placename};
	</select>
	
	<!-- 여행 계획 데이타 가져오기 -->
	<select id="getTripData" parameterType="String" resultType="trip">
		select * from trip where id=#{id}
	</select>
	
	
	<!-- <select id="getCityList" resultType="city">
		select * from city
	</select> -->
</mapper>